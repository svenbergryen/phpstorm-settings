<application>
  <component name="XDebuggerSettings">
    <data-views>
      <sort-values>true</sort-values>
      <option name="valueLookupDelay" value="200" />
    </data-views>
    <general>
      <confirm-breakpoint-removal>true</confirm-breakpoint-removal>
      <option name="hideDebuggerOnProcessTermination" value="true" />
    </general>
    <debuggers>
      <debugger id="javascript">
        <configuration doNotStepIntoLibraryCode="false">
          <custom-object-presentation />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>